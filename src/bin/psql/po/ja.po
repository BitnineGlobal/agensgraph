msgid ""
"  PGPORT\n"
"    same as the port connection parameter\n"
msgstr ""
"  PGPORT\n"
"    port 接続パラメータと同じ\n"

#: help.c:490
#, c-format
msgid ""
"  PGUSER\n"
"    same as the user connection parameter\n"
msgstr ""
"  PGUSER\n"
"    user 接続パラメータと同じ\n"

#: help.c:492
#, c-format
