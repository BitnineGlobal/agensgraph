#: help.c:243
#, c-format
msgid "  \\dg[S+] [PATTERN]      list roles\n"
msgstr "  \\dg[S+] [МАСКА]        список ролей\n"

#: help.c:244
#, c-format
msgid "  \\di[S+] [PATTERN]      list indexes\n"
msgstr "  \\di[S+] [МАСКА]        список индексов\n"

#: help.c:245
#, c-format
