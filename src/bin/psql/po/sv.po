"  psql --pset=NAMN[=VÄRDE]\n"
"  eller \\pset NAMN [VÄRDE] inne i psql\n"
"\n"

#: help.c:428
#, c-format
msgid ""
"  border\n"
"    border style (number)\n"
msgstr ""
"  border\n"
"    ramstil (nummer)\n"

#: help.c:430
#, c-format
msgid ""
"  columns\n"
"    target width for the wrapped format\n"
msgstr ""
"  columns\n"
"    målvidd för wrappade format\n"

#: help.c:432
#, c-format
msgid ""
"  expanded (or x)\n"
"    expanded output [on, off, auto]\n"
msgstr ""
"  expanded (eller x)\n"
"    expanderad utdata [on, off, auto]\n"

#: help.c:434
#, c-format
msgid ""
"  fieldsep\n"
"    field separator for unaligned output (default \"%s\")\n"
msgstr ""
"  fieldsep\n"
"    fältseparator för ej justerad utdata (standard \"%s\")\n"

#: help.c:437
#, c-format
msgid ""
"  fieldsep_zero\n"
"    set field separator for unaligned output to a zero byte\n"
msgstr ""
"  fieldsep_zero\n"
"    sätt fältseparator för ej justerad utdata till noll-byte\n"

